{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "nixpacksVersion": "1.21.2",
        "builder": "NIXPACKS"
    }
    "deploy": {
    "services": [
      {
        "name": "frontend", 
        "source": "client", 
        "startCommand": "npm run start" 
      },
      {
        "name": "backend",  
        "source": "server", 
        "startCommand": "python app.py"  // Or your specific Python start command
      }
    ]
  }
}
